# AutoDevHub Environment Variables
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=admin
DB_PASSWORD=password
DB_NAME=autodevhub

# API Settings
API_HOST=http://localhost
API_PORT=8000
API_KEY=your_api_key
API_SECRET=your_api_secret

# Authentication
AUTH_USERNAME=admin
AUTH_PASSWORD=password
AUTH_TOKEN_EXPIRY=3600

# Terraform Settings
TF_STATE_BUCKET=autodevhub-tfstate
TF_REGION=us-west-2
TF_ACCESS_KEY=YOUR_AWS_ACCESS_KEY
TF_SECRET_KEY=YOUR_AWS_SECRET_KEY

# Python Settings
PYTHONPATH=/path/to/python

# TypeScript Settings
TS_NODE_COMPILER_OPTIONS={"module":"commonjs"}

# Logging
LOG_LEVEL=INFO
LOG_FILE=autodevhub.log

# Other Settings
DEBUG=True
TIMEZONE=UTC
VERSION=1.0.0

# Email Settings
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USERNAME=your_email
EMAIL_PASSWORD=your_password

# GitHub Settings
GITHUB_TOKEN=your_github_token
GITHUB_USERNAME=your_github_username
GITHUB_REPO=autodevhub

# Git Settings
GIT_USERNAME=your_git_username
GIT_EMAIL=your_git_email

# AWS Settings
AWS_ACCESS_KEY=YOUR_AWS_ACCESS_KEY
AWS_SECRET_KEY=YOUR_AWS_SECRET_KEY
AWS_REGION=us-west-2

# Google Cloud Settings
GOOGLE_CLOUD_PROJECT=autodevhub
GOOGLE_CLOUD_KEY_FILE=/path/to/key.json

# Docker Settings
DOCKER_HOST=localhost
DOCKER_PORT=2375
DOCKER_USERNAME=your_docker_username
DOCKER_PASSWORD=your_docker_password

# Kubernetes Settings
KUBECONFIG=/path/to/kubeconfig
KUBE_CONTEXT=autodevhub

# Sentry Settings
SENTRY_DSN=https://sentry.io/your_sentry_dsn
SENTRY_ENV=production

# Redis Settings
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# Celery Settings
CELERY_BROKER_URL=amqp://guest:guest@localhost:5672//
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# Mailgun Settings
MAILGUN_API_KEY=your_mailgun_api_key
MAILGUN_DOMAIN=your_mailgun_domain

# Stripe Settings
STRIPE_API_KEY=your_stripe_api_key
STRIPE_SECRET_KEY=your_stripe_secret_key

# Twilio Settings
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number